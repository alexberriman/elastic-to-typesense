{
  "name": "elasticsearch-to-typesense",
  "version": "0.0.0-development",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf dist && tsc",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:integration": "vitest --config vitest.integration.config.ts run",
    "lint": "eslint 'src/**/*.ts' --ignore-pattern '*.test.ts'",
    "lint:tests": "eslint 'src/**/*.test.ts'",
    "lint:all": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "format:check": "prettier --check 'src/**/*.{js,ts}'",
    "check": "npm run lint && npm run format:check && npm run test",
    "prepare": "npm run build:clean",
    "prepublishOnly": "npm run check",
    "postinstall": "node ./scripts/postinstall.js || true"
  },
  "keywords": [
    "elasticsearch",
    "typesense",
    "search",
    "query",
    "conversion"
  ],
  "author": "Alex Berriman <alexb@bezz.com.au>",
  "license": "ISC",
  "description": "Convert Elasticsearch queries to Typesense format",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexberriman/elasticsearch-to-typesense.git"
  },
  "files": [
    "dist",
    "scripts",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3",
    "semantic-release": "^22.0.12",
    "typescript": "^5.8.3",
    "typesense": "^2.0.3",
    "vitest": "^3.1.1"
  }
}
